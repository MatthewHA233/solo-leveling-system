{
  "apps": [
    {
      "name": "solo-leveling",
      "script": "python3",
      "args": "-m server.core",
      "cwd": "/root/projects/solo-leveling-system",
      "interpreter": "none",
      "env": {
        "PYTHONUNBUFFERED": "1"
      },
      "max_restarts": 10,
      "restart_delay": 3000,
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
      "error_file": "/root/projects/solo-leveling-system/data/logs/error.log",
      "out_file": "/root/projects/solo-leveling-system/data/logs/out.log"
    }
  ]
}
